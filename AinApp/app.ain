تصنيف طالب{

	متغير العمر=0L
	
	إنشاء(){}
	إنشاء(العمر:كبير){
		هذا.العمر=العمر
	}
	دالة مؤثر ضرب(ر:صحيح):طالب{
		العمر*=ر
		
		أرجع إنشاء طالب(50L)
	}
	
	دالة مؤثر زد():طالب{
		العمر++
		أرجع إنشاء طالب(60L)
	}
}

دالة البداية(){
	حجز_مساحة_الطباعة()
	
	متغير م=إنشاء طالب[5UL]
	متغير ن=0UL
	متغير ت=20L
	
	طالما(ن<م.السعة)
		م[ن++]=إنشاء طالب(ت++)
	
	م[0UL]*=2
	طباعة_(م[0UL].العمر)
	
	طباعة_((++م[0UL]).العمر)
	
	طباعة_(++م[0UL].العمر)
	
	/*
	ت=0L
	طباعة_(++ت.زد())
	*/
}