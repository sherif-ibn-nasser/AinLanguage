تخصيص متغير أول_عنوان=0L
تخصيص متغير آخر_عنوان=0L

دالة حجز_مساحة_الطباعة(){
	لو(أول_عنوان!=0L){أرجع}
	
	إزاحة_الذاكرة(0L)
	آخر_عنوان=إزاحة_الذاكرة(8*20L)
}
